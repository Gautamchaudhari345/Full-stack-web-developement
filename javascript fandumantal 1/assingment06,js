let name = "Steve";
let email = "Steve@gmail.com";
let age = "23";
// let age = 23;

// Checking Conditions
if (typeof(name) != typeof ('string')) {
    console.log("Name Should Be A String");
}

if (typeof(email) != typeof ('string')) {
    console.log("Email Should Be A String");
}

if (typeof(age) != typeof (123)) {
    console.log("Age Should Be A Number");
}

console.log("All Conditions Compared Successfully");